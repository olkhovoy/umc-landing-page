# ===== UMC LANDING PAGE ENVIRONMENT VARIABLES =====

# IPFS Configuration
# Set to 'true' to enable IPFS functionality
ENABLE_IPFS=true

# Pinata Configuration (for content pinning)
# Get your API key from https://app.pinata.cloud/
PINATA_TOKEN=your_pinata_api_key_here

# Infura Configuration (alternative pinning service)
# Get your API key from https://infura.io/
INFURA_TOKEN=your_infura_api_key_here

# ENS Domain (future feature)
# Your .eth domain for the landing page
ENS_DOMAIN=umc.eth

# Environment
NODE_ENV=production

# Analytics (optional)
# Google Analytics ID
GA_ID=your_google_analytics_id_here

# Social Media
# Twitter handle for social sharing
TWITTER_HANDLE=@umc_theory

# GitHub repository
GITHUB_REPO=https://github.com/olkhovoy/umc-landing-page

# Contact information
CONTACT_EMAIL=contact@umc.eth 